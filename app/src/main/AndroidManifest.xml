<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.googlematerialdesgin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".tab_layout.TabLayoutActivity"
            android:theme="@style/TabLayoutTheme"
            >

        </activity>
        <activity
            android:name=".bottom_navigation_bar.BottomNavigationBarActivity"
            android:theme="@style/AppTheme.TopAppBar.Themed"></activity>
        <activity
            android:name=".top_app_bar.TopAppBarActivity"
            android:theme="@style/AppTheme.TopAppBar.Themed" />
        <activity
            android:name=".bottom_sheet_bar.BottomBarActivity"
            android:theme="@style/AppTheme.BottomAppBar.Themed" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>